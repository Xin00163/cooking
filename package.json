{
    "name": "sg",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "node ./bin/www",
        "build": "babel src -d build",
        "babel-node": "babel-node",
        "start-dev": "nodemon --exec npm run babel-node -- ./website/src/start",
        "lint": "eslint src",
        "lintfix": "eslint src --fix",
        "drop": "mongo < ./website/test/restore-db-script.js",
        "restore": "mongorestore",
        "reset": "npm run drop && npm run restore",
        "droprestore": "mongorestore --drop",
        "test": "wdio ./website/wdio.conf.js",
        "cleantest": "npm run droprestore && npm test"
    },
    "dependencies": {
        "bluebird": "^3.5.3",
        "body-parser": "^1.18.3",
        "browserify": "^16.2.3",
        "cookie-parser": "~1.3.5",
        "debug": "^4.1.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "express": "^4.16.4",
        "i": "^0.3.6",
        "lodash": "^4.17.11",
        "mongodb": "^3.1.10",
        "mongoose": "^5.3.15",
        "mongoskin": "1.3.20",
        "monk": "^3.1.2",
        "morgan": "^1.9.1",
        "npm": "^6.4.1",
        "pug": "^2.0.3",
        "pug-bootstrap": "0.0.15",
        "request": "^2.88.0",
        "serve-favicon": "^2.5.0",
        "superagent": "^3.8.3",
        "superagent-mock": "^3.7.0"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-install": "^2.1.0",
        "babel-preset-env": "^1.7.0",
        "chai": "^4.2.0",
        "chromedriver": "^2.44.1",
        "concurrently": "^3.6.1",
        "eslint": "^4.19.1",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-jsx-a11y": "^6.1.1",
        "mocha": "^5.2.0",
        "node-firefox-driver": "^1.0.0",
        "nodemon": "^1.18.7",
        "wdio-chromedriver-service": "^0.1.5",
        "wdio-mocha-framework": "^0.5.13",
        "wdio-selenium-standalone-service": "0.0.10",
        "wdio-spec-reporter": "^0.1.5",
        "webdriverio": "^4.14.1"
    }
}
